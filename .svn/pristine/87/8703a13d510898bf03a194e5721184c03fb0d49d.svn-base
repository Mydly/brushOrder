
import React, { PropTypes, Component } from 'react'
import {Table} from 'react-bootstrap';

var List =function(props){
    return (
        <tbody>
        {props.children.map((child, i) =>
            <tr >
                {child.map((child2, i) =>
                    <Tdcell data={child2}/>
                )}
            </tr>
        )}
        </tbody>
    )
}


export  class Tdcell extends Component {
    render(){
        if(typeof (this.props.data) == 'object') {
            var  cls=this.props.data[0];
            var txt=this.props.data[1];
            var funct=function(){};
            if( this.props.data[2]) {
                var funct = this.props.data[2];
                var id    = this.props.data[3];
            }
        }else
        {
            var  cls='';
            var  txt=this.props.data;
            var funct=function(){};
        }
        return (
            <td className={cls} onClick={()=> {funct(id)}}>{txt}</td>
        )
    }
}

Tdcell.propType = {
    data:PropTypes.object
}




export default class Log extends Component {

    render(){
        return (
            <div>
<Table striped condensed hover className="tab">
    <thead>
    <tr>
        {this.props.name}
    </tr>
    </thead>
    <List  children = {this.props.data}  />
</Table>
            </div>
        );
    }
}

