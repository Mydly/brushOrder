import React from 'react'
import '../../static/style/common.scss'
import {  HashRouter as Router, Route, Link} from 'react-router-dom'


import Nav from './Nav';
import Index from './Index';
import Trade from './Trade';
import Safe from './Safe';
import Finan from './Finan';
import Issue from './Issue';
import Money from './Money';
import Vote from './Vote';

class Content extends React.Component {

    constructor(props) {
        super(props);
        //this.handleClick = this.handleClick.bind(this);
    }

    render() {
        //const { dispatch, visibleTodos, visibilityFilter } = this.props;
        //console.log(visibleTodos);
        let menuArr = [
            {title:"首页",key:"index"},
            {title:"交易中心",key:"trade"},
            {title:"安全中心",key:"safe",child:[
                {title:"账户预览",key:"safe/account"},
                {title:"登录密码",key:"safe/psd"},
                {title:"收货地址",key:"safe/address"}
            ]},
            {title:"财务中心",key:"finan"},
            {title:"认购中心",key:"issue"},
            {title:"理财中心",key:"money"},
            {title:"新币投票",key:"vote"}];
        return (
            <div>
            <Route path="index" component={Index}></Route>
            <Route path="trade" component={Trade}></Route>
            <Route path="safe" component={Safe}></Route>
            <Route path="finan" component={Finan}></Route>
            <Route path="issue" component={Issue}></Route>
            <Route path="money" component={Money}></Route>
            <Route path="vote" component={Vote}></Route>
            </div>
        );
    }
}

export default Content;