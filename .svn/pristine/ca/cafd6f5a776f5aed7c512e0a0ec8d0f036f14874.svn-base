import React, { Component } from 'react'
import Log from '../issue/Log';

var ret= [
    {
        id: 2,
        userid: 1,
        invit: 1,
        type: "认购赠送",
        name: "一代认购赠送",
        num: "1000.00000000",
        invitname: "qq83839140",
        fee: "10.00000000",
        feecoin: "cny",
        sort: 0,
        addtime: "2016-12-22 15:02:17",
        endtime: 0,
        status: 1
    },
    {
        id: 1,
        userid: 1,
        invit: 1,
        type: "认购赠送",
        name: "赠送自己",
        num: "1000.00000000",
        invitname: "qq83839140",
        fee: "10.00000000",
        feecoin: "cny",
        sort: 0,
        addtime: "2016-12-22 15:02:17",
        endtime: 0,
        status: 1
    }
]

export default class InvitLog extends React.Component {
    render() {
        var data=[];
        for (var i in ret){
             data[i]=[ret[i].invitname,ret[i].type,ret[i].name,ret[i].addtime,ret[i].num,ret[i].fee,ret[i].feecoin];
             data[i].push(ret[i].jd+'/'+ret[i].ci);
            data[i].push( ret[i].shen);
            if(ret[i].status==0) {
                var ele=['red',"未到账"]
                data[i].push(ele)
            }
            else{
                var ele=['green',"已到账"]
                data[i].push(ele)
            }
        }
        var tharr=[
                <th>下家用户名</th>,
                <th>奖励类型</th>,
                <th>奖励说明</th>,
                <th>操作时间</th>,
                <th>操作数量</th>,
                <th>奖励数量</th>,
                <th>奖励币种</th>,
                <th>奖励状态</th>
        ]
        return (
            <div>
                <Log name={tharr} data={data} />
            </div>
        );
    }
}



