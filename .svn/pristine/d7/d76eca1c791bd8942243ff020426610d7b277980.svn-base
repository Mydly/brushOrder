import React from 'react';
import {Nav,NavItem} from 'react-bootstrap';
import CONSTS from '../../config/consts';

class Finan extends React.Component {


    constructor(props) {
        super(props);

        this.__clickItem = this.__clickItem.bind(this);
        //this.__changeTradeArea = this.__changeTradeArea.bind(this);

        this.state = {
            itemIndex:CONSTS.pageKey.finan[0].key
        }

    }

    __clickItem(index,e){
        console.log(index);
        this.setState({
           itemIndex: index
        });
    }

    render() {
        //const { dispatch, visibleTodos, visibilityFilter } = this.props;
        console.log('finan render');


        const style = {
            display: 'inline-block',
            margin: '16px 32px 16px 0',
        };

        let mentlist = CONSTS.pageKey.finan.map(
            (it)=>{
                return <NavItem key={it.key} eventKey={it.key}>{it.title}</NavItem>
            }
        );

        let menu = <Nav  style={{'width':'160px',backgroundColor:'#0fef00'}}
                         activeKey={this.state.itemIndex}
                         onSelect={this.__clickItem}>
            {mentlist}
            </Nav>;

        let content = <div>{this.state.itemIndex}</div>;

        return (
            <div>
                {menu}
                {content}
            </div>
        );
    }
}

export default Finan;