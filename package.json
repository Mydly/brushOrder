{
  "name": "brush-order",
  "version": "1.0.0",
  "description": "brush order tool",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron ./app",
    "dev": "electron ./app",
    "watch": "webpack --watch --color --config webpack.config.js",
    "builder": "electron-builder --dir==./app --config=./build.json",
    "build_all": "yarn run builder --mac && yarn run builder --win --x64",
    "build_win": "yarn run builder --win --x64",
    "build_mac": "yarn run builder --mac ",
    "pack_all": "electron-packager ./app brushOrder --overwrite --asar --all --arch=x64 --icon=./icon.ico --out=./dist --app-version=2.0.1 --ignore='(dist|docs|.gitignore|LICENSE|README.md)'",
    "pack_mac": "electron-packager ./app brushOrder --overwrite --platform=darwin --arch=x64 --out=./dist --ignore='(dist|docs|.gitignore|LICENSE|README.md)'",
    "pack_win": "electron-packager ./app brushOrder --overwrite --asar --platform=win32 --arch=x64 --electronVersion=1.7.9 --out=./dist --app-version=1.0.1 --ignore='(dist|docs|.gitignore|LICENSE|README.md)'",
    "pack_linux": "electron-packager ./app brushOrder --overwrite --asar --platform=linux --arch=x64 --icon=./icon.ico --out=./dist --app-version=2.0.1 --ignore='(dist|docs|.gitignore|LICENSE|README.md)'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mydly/brush-order-tool.git"
  },
  "keywords": [
    "request,tool,debug,test api"
  ],
  "author": "Mydly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mydly/brush-order-tool/issues"
  },
  "homepage": "https://github.com/Mydly/brush-order-tool#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.7.2",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "electron": "^1.7.9",
    "electron-builder": "^19.48.3",
    "electron-packager": "^10.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "less-loader": "^2.2.2",
    "n": "^2.1.8",
    "node-pre-gyp": "^0.6.39",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "sass-loader": "^6.0.3",
    "scss": "^0.2.4",
    "style-loader": "^0.19.0",
    "url-loader": "^0.5.9"
  },
  "dependencies": {
    "antd": "^2.13.9",
    "babel-plugin-import": "^1.6.2",
    "better-sqlite3": "^4.1.0",
    "check": "^1.0.0",
    "electron-settings": "^3.1.4",
    "material-ui": "^0.18.7",
    "mydly-check": "1.1.1",
    "mydly-request": "^1.0.1",
    "node-rsa": "^0.4.2",
    "node-sass": "^4.7.2",
    "qrcode.react": "^0.7.2",
    "react-bootstrap": "^0.31.2",
    "react-icons": "^2.2.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "request": "2.79.0",
    "webpack": "^3.10.0"
  }
}
